List1[-1].split(",")





# Copy referance
nL = ["Bad", 32, "Cat", 96]
print("Before clone : ", nL)
CnL = nL
print("After clone : ", CnL)


nL[0] = "Dog"
print("nL: ", nL)
print("CnL: ", CnL)


# Clone 
nL = CnL[:]
nL[0] = "Ret"
print("nL: ", nL)
print("CnL: ", CnL)






