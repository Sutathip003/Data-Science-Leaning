


# Lists operations
List = ["A dog", 3.2, 1998, True,  [1, 2], ("A", 1)]
print("Positive index : ", List[0], "\nNegative index",List[-6])


#List slicing
slice_list = List[1:3]
print(slice_list)


# Extend list
List2 = ["cat", False]
List.extend(List2)
print(List)


# Append list
List2 = ["cat", False]
List.append(List2)
print(List)


# check list length
len_list = len(List)
print(len_list)


# Change the element
