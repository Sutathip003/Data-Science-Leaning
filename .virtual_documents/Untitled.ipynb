arr1 = np.array([1, 2, 3, 4, 5])
arr2 = np.array([6, 7, 8, 9, 10])

EvenArr1 = []
OddArr1= []
for i in arr1:
    if i % 2 == 0:
        EvenArr1.append(i)       
    else:
        OddArr1.append(i)
        
print("Even number of arr1 : ", EvenArr1)
print("Odd number of arr1 : ", OddArr1)

print("____________________________________")

